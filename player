int x= 160;
int y= 275;
int AsX = random(0,320);
int AsY = 50;
void main(){
dibuja();
}

void dibuja(){
  animate(ejecutar);
}

void ejecutar(){
  player();
  asteroide();
}  
 
void player(){
   image("https://static.vecteezy.com/system/resources/previews/046/807/271/non_2x/pixel-art-illustration-space-background-pixelated-space-background-space-background-nebula-pixelated-for-the-pixel-art-game-and-icon-for-website-and-game-old-school-retro-vector.jpg",0,0,320,320); 
 image ("https://i.pinimg.com/originals/5d/54/2f/5d542f9a2c4b6591b4104266d1786c18.png",mouseX,y,35,35);

  }
 
void asteroide(){
  ellipse(AsX,AsY,40,40);
  fill(86,64,9);
  if (AsX == random (0,320));
  	AsY= (320);
}
